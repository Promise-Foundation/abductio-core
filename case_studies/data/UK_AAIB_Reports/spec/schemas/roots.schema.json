{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["case_id", "root_set_id", "roots"],
  "properties": {
    "case_id": {"type": "string"},
    "root_set_id": {"type": "string"},
    "roots": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id"],
        "properties": {
          "id": {"type": "string"},
          "nec_slots": {"type": "array"}
        }
      }
    }
  }
}
